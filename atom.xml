ass="line"><span class="keyword">case</span> <span class="number">0xd0</span>:</span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_0);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0xde</span>)</span><br><span class="line">key_num=<span class="number">2</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_1);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"></span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0xdd</span>)</span><br><span class="line">key_num=<span class="number">6</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_2);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"></span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0xdb</span>)</span><br><span class="line">key_num=<span class="number">10</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">key_num=<span class="number">14</span>;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line"><span class="keyword">break</span>;</span><br><span class="line"><span class="keyword">case</span> <span class="number">0xb0</span>:</span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_0);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0xbe</span>)</span><br><span class="line">key_num=<span class="number">3</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_1);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"></span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0xbd</span>)</span><br><span class="line">key_num=<span class="number">7</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_2);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"></span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0xbb</span>)</span><br><span class="line">key_num=<span class="number">11</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">key_num=<span class="number">15</span>;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line"><span class="keyword">break</span>;</span><br><span class="line"><span class="keyword">case</span> <span class="number">0x70</span>:</span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_0);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=(GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>);</span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0x7e</span>)</span><br><span class="line">key_num=<span class="number">4</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_1);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"></span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0x7d</span>)</span><br><span class="line">key_num=<span class="number">8</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">&#123;</span><br><span class="line">GPIO_SetBits(GPIOF,GPIO_Pin_1|GPIO_Pin_2|GPIO_Pin_3|GPIO_Pin_0);</span><br><span class="line">GPIO_ResetBits(GPIOF,GPIO_Pin_2);</span><br><span class="line">delay_us(<span class="number">1</span>);</span><br><span class="line">temp1=GPIO_ReadInputData(GPIOF)&amp;<span class="number">0xff</span>;</span><br><span class="line"></span><br><span class="line"><span class="keyword">if</span> (temp1==<span class="number">0x7b</span>)</span><br><span class="line">key_num=<span class="number">12</span>;</span><br><span class="line"><span class="keyword">else</span></span><br><span class="line">key_num=<span class="number">16</span>;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line"><span class="keyword">break</span>;</span><br><span class="line"><span class="keyword">default</span> :<span class="keyword">return</span> <span class="number">0</span>;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line">&#125;</span><br><span class="line"><span class="keyword">else</span> <span class="keyword">if</span>(temp == <span class="number">0xf0</span>)</span><br><span class="line">&#123;</span><br><span class="line">key_up=<span class="number">1</span>;<span class="comment">//当没有键按下后，将连按标志复位</span></span><br><span class="line">&#125;</span><br><span class="line"><span class="keyword">return</span> key_num;</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>]]></content>
    
    
